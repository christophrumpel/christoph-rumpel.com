{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "bepsvpt/secure-headers": "^7.2",
        "doctrine/dbal": "^3.0",
        "facade/ignition": "^2.15",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^7.29",
        "laravel/horizon": "^5.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.5",
        "livewire/livewire": "^2.10",
        "owenvoke/blade-fontawesome": "^1.7",
        "spatie/commonmark-highlighter": "^3.0",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-feed": "^3.2",
        "spatie/laravel-honeypot": "^3.0",
        "spatie/laravel-mailcoach": "^4.0",
        "spatie/laravel-mailcoach-postmark-feedback": "^3.0",
        "spatie/laravel-mailcoach-unlayer": "^2.0",
        "spatie/laravel-responsecache": "^7.1",
        "spatie/laravel-sitemap": "^5.8",
        "spatie/laravel-welcome-notification": "^2.3",
        "spatie/schema-org": "^3.11",
        "spatie/yaml-front-matter": "^2.0",
        "wildbit/swiftmailer-postmark": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "fakerphp/faker": "^1.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag mailcoach-assets --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
